parameters:
    level: max
    checkMissingIterableValueType: false
    magento:
        checkServiceContracts: false
    fileExtensions:
        - php
    paths:
        - Api
        - Block
        - Console
        - Handler
        - Model
        - Observer
        - Test
    bootstrapFiles:
        - vendor/bitexpert/phpstan-magento/autoload.php
